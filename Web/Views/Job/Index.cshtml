                                    </div>
                                    <div class="am-u-sm-5">
                                        <input type="date" id="d2" name="end" value=@ViewBag.Project.End placeholder="项目结束日期" data-am-datepicker>
                                    </div>
                                </div>
                                <div class="am-form-group">
                                    <label for="d3" class="am-u-sm-2 am-form-label">公司名称：</label>
                                    <div class="am-u-sm-10">
                                        <input type="text" id="d3" name="company" value=@ViewBag.Project.Company placeholder="输入公司名称">
                                    </div>
                                </div>
                                <div class="am-form-group">
                                    <label for="d4" class="am-u-sm-2 am-form-label">项目名称：</label>
                                    <div class="am-u-sm-10">
                                        <input type="text" id="d4" name="name" value=@ViewBag.Project.Name placeholder="输入项目名称">
                                    </div>
                                </div>
                                <div class="am-form-group">
                                    <label for="d5" class="am-u-sm-2 am-form-label">担任职位：</label>
                                    <div class="am-u-sm-10">
                                        <select id="d5" data-am-selected="{btnWidth: '40%', btnSize: 'sm', btnStyle: 'default'}" name="Position" value=@ViewBag.Project.Position>
                                            <option value="项目经理">项目经理</option>
                                            <option value="产品经理">产品经理</option>
                                            <option value="全职">全职</option>
                                            <option value="实习">实习</option>
                                            <option value="打杂">打杂</option>
                                            <option value="兼职">兼职</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="am-form-group">
                                    <label for="d6" class="am-u-sm-2 am-form-label">项目描述：</label>
                                    <div class="am-u-sm-10">
                                        <textarea class="" rows="3" id="d6" name="descript">@ViewBag.Project.Descript</textarea>
                                    </div>
                                </div>
                                <div class="am-form-group">
                                    <div class="am-u-sm-10 am-u-sm-offset-2">
                                        <button id="addProject" class="am-btn am-btn-default">保存</button>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
                <div data-am-widget="list_news" class="am-list-news am-list-news-default">
                    <div class="am-list-news-bd">
                        <ul class="am-list">
                        @foreach (var project in @ViewBag.Projects)
                        {
                            <li id=@project.ID class="am-list-item-desced">
                                <div class="am-list-item-hd">
                                    <h4>
                                        <span>@project.Begin — @project.End</span>
                                        <span class="am-text-success">@project.Company</span>
                                        <button class="am-btn am-btn-default am-btn-xs" style="float:right;">
                                            <i class="am-icon-remove remove-project" project=@project.ID ></i>
                                        </button>
                                        <button class="am-btn am-btn-default am-btn-xs" style="float:right;">
                                            <i class="am-icon-edit"></i>
                                        </button>
                                    </h4>
                                </div>
                                <div class="am-list-item-text">
                                    <p>
                                        <span>@project.Position</span>
                                        <span>@project.Name</span>
                                        <span>@project.Descript</span>
                                    </p>
                                </div>
                            </li>
                        }
                        </ul>
                    </div>
                </div>
            </div>
        </div>
