                                    </div>
                                    <label for="inputSex" class="am-u-sm-2 am-form-label">性别：</label>
                                    <div class="am-u-sm-4">
                                        <select data-am-selected name="sex" required value=@ViewBag.Basic.Sex>
                                            <option value="male">男</option>
                                            <option value="female">女</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="am-form-group">
                                    <label for="inputSchool" class="am-u-sm-2 am-form-label">学校：</label>
                                    <div class="am-u-sm-4">
                                        <input type="text" id="inputSchool" placeholder="输入你必要的学校" name="school" value=@ViewBag.Basic.School>
                                    </div>
                                    <label for="inputEducation" class="am-u-sm-2 am-form-label" onabort="education">学历：</label>
                                    <div class="am-u-sm-4">
                                        <select data-am-selected name="education" value=@ViewBag.Basic.Education>
                                            <option value="A">博士</option>
                                            <option value="B">硕士</option>
                                            <option value="C">本科</option>
                                            <option value="D">大专</option>
                                            <option value="E">高中</option>
                                            <option value="F">初中</option>
                                            <option value="G">小学</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="am-form-group">
