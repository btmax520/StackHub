                                        <select data-am-selected name="kind" value=@ViewBag.Basic.Kind>
                                            <option value="1">全职</option>
                                            <option value="2">实习</option>
                                            <option value="3" selected>兼职</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="am-form-group">
