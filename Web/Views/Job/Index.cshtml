                                    <div class="am-u-sm-4">
                                        <input type="date" id="inputBirthday" placeholder="输入你的生日" name="birthday" data-am-datepicker required value=@ViewBag.Basic.Birthday>
                                    </div>
                                    <label for="inputExperience" class="am-u-sm-2 am-form-label">经验：</label>
                                    <div class="am-u-sm-4">
                                        <select data-am-selected name="experience" value=@ViewBag.Basic.Experience>
                                            <option value="A">应届毕业生</option>
                                            <option value="B">实习生</option>
                                            <option value="1">1年</option>
                                            <option value="2">2年</option>
                                            <option value="3">3年</option>
                                            <option value="4">4年</option>
                                            <option value="5">5年</option>
